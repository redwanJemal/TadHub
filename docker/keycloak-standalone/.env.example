# Keycloak Standalone Environment Variables
# Copy to .env and fill in values

# =============================================================================
# PostgreSQL Database
# =============================================================================
KC_DB_USER=keycloak
KC_DB_PASSWORD=<strong-password-here>
KC_DB_NAME=keycloak

# =============================================================================
# Keycloak Admin
# IMPORTANT: Change password after first login!
# =============================================================================
KC_ADMIN_USER=admin
KC_ADMIN_PASSWORD=<strong-password-here>

# =============================================================================
# SSL/TLS
# =============================================================================
ACME_EMAIL=admin@endlessmaker.com
