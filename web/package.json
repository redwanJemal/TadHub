{
  "name": "modular-saas-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:tenant": "pnpm --filter tenant-app dev",
    "dev:backoffice": "pnpm --filter backoffice-app dev",
    "build": "pnpm build:tenant && pnpm build:backoffice",
    "build:tenant": "pnpm --filter tenant-app build",
    "build:backoffice": "pnpm --filter backoffice-app build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
